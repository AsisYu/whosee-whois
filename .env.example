# API配置
# 这些变量用于构建API端点URL
# 格式: ${VITE_API_PROTOCOL}://${VITE_API_HOST}:${VITE_API_PORT}
VITE_API_PROTOCOL=http  # API服务器协议: http 或 https
VITE_API_HOST=localhost  # API服务器主机名
VITE_API_PORT=3900  # API服务器端口号，留空表示使用默认端口(http:80, https:443)

# 本地开发配置示例
# VITE_API_PROTOCOL=http
# VITE_API_HOST=localhost
# VITE_API_PORT=3900

# 认证配置
VITE_API_TOKEN=xxx  # API认证令牌

# 缓存配置
VITE_CACHE_ENABLED=true  # 是否启用缓存
VITE_CACHE_TTL=1800000  # 缓存有效期(毫秒)，默认30分钟

# 其他配置
VITE_DEBUG_MODE=false  # 是否启用调试模式
